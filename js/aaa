SET OPENSSL_HOME=C:\openssl-0.9.8k_X64
SET OPENSSL_CONF=%OPENSSL_HOME%\openssl.cnf
SET PATH=%PATH%;%OPENSSL_HOME%\bin
openssl

openssl genrsa -aes256 -out private.key 2048

openssl rsa -in private.key -pubout -out public.key

openssl req -new -key private.key -out private.csr




genrsa -out ./test/private.key 2048

rsa -in ./test/private.key -pubout -out ./test/public.key

req -new -key ./test/private.key -config ./openssl.cnf -out ./test/private.csr

req -x509 -days 365 -key ./test/private.key -in ./test/private.csr -out ./test/private.crt -days 365

pkcs12 -export -in ./test/private.crt -inkey ./test/private.key -out ./test/.keystore -name tomcat