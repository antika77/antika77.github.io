genrsa -des3 -out ./test/private.pem 2048

genrsa -out ./test/private.key 2048

rsa -in ./test/private.key -pubout -out ./test/public.key

req -new -key ./test/private.key -config ./openssl.cnf -out ./test/private.csr

//req -new -key ./test/private.key -out ./test/private.csr

genrsa -aes256 -out ./test/rootCA.key 2048

req -x509 -new -nodes -key ./test/rootCA.key -days 3650 -config ./openssl.cnf -out ./test/rootCA.pem

x509 -req -in ./test/private.csr -CA ./test/rootCA.pem -CAkey ./test/rootCA.key -CAcreateserial -out ./test/private.crt -days 3650

pkcs12 -export -in ./test/private.crt -inkey ./test/private.key -out ./test/.keystore -name tomcat





genrsa -aes256 -out ./test2/private.key 2048

req -new -key ./test2/private.key -out ./test2/rootca.csr -config ./openssl.cnf

x509 -req -days 3650 -extensions v3_ca -set_serial 1 -in ./test2/rootca.csr -signkey ./test2/private.key -out ./test2/rootca.crt

genrsa -aes256 -out ./test2/private.key 2048

openssl req -new -key ./test2/private.key -out ./test2/localhost.csr -config ./openssl.cnf




genrsa -aes256 -out rootCA.key 2048

req -x509 -new -nodes -key rootCA.key -days 3650 -out rootCA.pem -config ../openssl.cnf

genrsa -out abrand.key 2048

req  -new -key abrand.key -out abrand.csr -config ../openssl.cnf

x509 -req -in abrand.csr -CA rootCA.pem -CAkey rootCA.key -CAcreateserial -out abrand.crt -days 3650

pkcs12 -export -in abrand.crt -inkey abrand.key -out .keystore -name tomcat


